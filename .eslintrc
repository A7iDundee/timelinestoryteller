{
    "extends": "eslint-config-airbnb-es5",
    "rules": {
        "no-var": 0,
        "quotes": [2, "double"],
        "camelcase": 0,
        "no-param-reassign": 0,
        "one-var": 0,
        "indent": [2, 4],
        "func-names": 0
    },
    "env": {
        "browser": true,
        "mocha": true
    }
}